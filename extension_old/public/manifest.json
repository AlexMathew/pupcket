{
  "manifest_version": 2,
  "name": "Pupcket",
  "author": "Alex Mathew",
  "version": "0.0.1",
  "description": "Alternate new tab page.",
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png",
    "192": "logo192.png",
    "512": "logo512.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["storage"]
}